{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch MinimalQmlApp",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/MinimalQmlApp",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "QT_QPA_PLATFORM",
                    "value": "wayland"
                },
                {
                    "name": "LANG",
                    "value": "ru_RU.UTF-8"
                },
                {
                    "name": "LC_ALL",
                    "value": "ru_RU.UTF-8"
                }
            ],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
